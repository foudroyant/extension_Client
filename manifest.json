{
    "manifest_version": 3,
    "name": "My Renta",
    "version": "1.0",
    "description": "Une extension de qui gère la rentabilité des biens sur la plateforme leboncoin.com",
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "host_permissions":[
      "<all_urls>"
    ],
    "permissions": [
      "tabs",
      "activeTab"
    ]
  }
  